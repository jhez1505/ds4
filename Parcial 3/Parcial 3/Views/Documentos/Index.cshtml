            @Html.DisplayNameFor(model => model.Notas)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CasoID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreDocumento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoDocumento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreArchivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RutaArchivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaSubida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notas)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DocumentoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DocumentoID }) |
